<?xml version="1.0" encoding="UTF-8"?>
<Configuration status = "ERROR">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{ABSOLUTE} %-5level %logger{1} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level = "ALL">
            <AppenderRef ref = "Console"/>
        </Root>
    </Loggers>
</Configuration>